# Indusa AI Try-On Service Configuration
# Copy this file to .env.local and fill in your API keys

# Clothing Try-On Services
VITE_KLING_API_URL=https://api.kling.ai
VITE_KLING_API_KEY=your_kling_api_key_here

VITE_REPLICATE_API_URL=https://api.replicate.com
VITE_REPLICATE_API_KEY=your_replicate_api_key_here

# Home Decor Visualization Services
VITE_REIMAGINE_API_URL=https://api.reimaginehome.ai
VITE_REIMAGINE_API_KEY=your_reimagine_home_api_key_here

VITE_MODSY_API_URL=https://api.modsy.com
VITE_MODSY_API_KEY=your_modsy_api_key_here

# AI Sizing Services
VITE_3DLOOK_API_URL=https://api.3dlook.me
VITE_3DLOOK_API_KEY=your_3dlook_api_key_here

VITE_BODYLABS_API_URL=https://api.bodylabs.com
VITE_BODYLABS_API_KEY=your_bodylabs_api_key_here

# Service Provider Selection (optional - defaults to mock services)
# Options: kling, replicate-tryon, mock-clothing
VITE_CLOTHING_PROVIDER=mock-clothing

# Options: reimagine-home, modsy, mock-decor
VITE_DECOR_PROVIDER=mock-decor

# Options: 3dlook, bodylabs, mock-sizing
VITE_SIZING_PROVIDER=mock-sizing

# Development Environment
NODE_ENV=development
